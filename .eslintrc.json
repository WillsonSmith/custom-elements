{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": [
    "@open-wc/eslint-config",
    "plugin:lit-a11y/recommended",
    "plugin:@typescript-eslint/recommended"
  ],
  "overrides": [],
  "parser": "@typescript-eslint/parser",
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "plugins": ["@typescript-eslint", "lit-a11y"],
  "rules": {
    "linebreak-style": ["error", "unix"]
  }
}
